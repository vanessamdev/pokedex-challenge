{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/treinadores(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/pokemons(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/batalhas(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}
